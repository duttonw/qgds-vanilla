
<!-- VERSION_DETAILS=###VERSION### -->

{{! Select environment, used verbatium if not using predefind key
cdn := PROD|STAGING|BETA|TEST|DEV|???
}}

{{~#*inline "cdnDomain" ~}}
    {{~#if cdn ~}}
        {{~#ifEqualsOrChained cdn 'PROD' 'STAGING' 'BETA' 'TEST' 'DEV' ~}}
            {{~#ifCond cdn '==' 'PROD'~}}
                https://static.qgov.net.au
                {{~else~}}
                {{~#ifCond cdn '==' 'STAGING'  ~}}
                    https://staging-static.qgov.net.au
                    {{~else~}}
                    {{~#ifCond cdn '==' 'BETA'~}}
                        https://beta-static.qgov.net.au
                    {{~else ~}}
                        {{~#ifCond cdn '==' 'TEST' ~}}
                            https://test-static.qgov.net.au
                        {{~else ~}}
                            https://dev-static.qgov.net.au
                        {{~/ifCond ~}}
                    {{~/ifCond ~}}
                {{~/ifCond ~}}
            {{~/ifCond ~}}
            /qgds-vanilla/###MAJOR_VERSION###/###MAJOR_VERSION###.x.x-latest
        {{~else ~}}
            {{~cdn ~}}
        {{~/ifEqualsOrChained ~}}
    {{~else ~}}
        missing
    {{~/if ~}}
{{~/inline ~}}

<link rel="stylesheet" href="{{> cdnDomain  cdn=@root.cdn}}/assets/css/main.css">

<script type="text/javascript" async src="{{> cdnDomain  cdn=@root.cdn}}/assets/js/main.min.js"></script>

