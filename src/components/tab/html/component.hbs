
{{#with component.data}}
<section class="qld__body{{#if metadata.theme.value}} qld__body--{{metadata.theme.value}}{{/if}}{{#ifCond metadata.tab_display.value '==' 'full'}} qld__body--full-width tab-section{{/ifCond}}" id="{{#if metadata.id_field.value}}{{metadata.id_field.value}}{{else}}tag-{{assetid}}{{/if}}">
    <div class="container-fluid">
        <div class="qld__tab-container{{#if metadata.theme.value}} qld__tab-container--{{metadata.theme.value}}{{/if}} qld__tab-container__fixed" id="tab-example-5">
            <button class="qld__tab-nav__item-scroll tab-overflow-nav-button-left" aria-hidden="true" aria-label="Scroll tab buttons left" tabindex="-1"><i class="fa-regular fa-chevron-left"></i></button>
            <button class="qld__tab-nav__item-scroll tab-overflow-nav-button-right" aria-hidden="true" aria-label="Scroll tab buttons right" tabindex="-1"><i class="fa-regular fa-chevron-right"></i></button>
            <header class="qld__tabs" role="tablist">

                <button class="qld__tab-button active" data-tab="tab1" aria-selected="true" aria-controls="tab1-content" tabindex="0" id="tab1-button"><span><i class="fa-light fa-gear"></i>Tab label 1</span></button>
                <button class="qld__tab-button" data-tab="tab2" aria-selected="false" aria-controls="tab2-content" tabindex="-1" id="tab2-button"><span><i class="fa-light fa-gear"></i>Tab label 2</span></button>
                <button class="qld__tab-button" data-tab="tab3" aria-selected="false" aria-controls="tab3-content" tabindex="-1" id="tab3-button"><span>Tab label 3</span></button>
                <button class="qld__tab-button" data-tab="tab4" aria-selected="false" aria-controls="tab4-content" tabindex="-1" id="tab4-button"><span>Tab label 4</span></button>
                <button class="qld__tab-button" data-tab="tab5" aria-selected="false" aria-controls="tab5-content" tabindex="-1" id="tab5-button"><span>Tab label 5</span></button>
            </header>
            <div data-tab="tab1" class="qld__tab-content active" role="tabpanel" aria-labelledby="tab1-button" id="tab1-content" tabindex="0" aria-hidden="false">Tab 1 content goes here</div>
            <div data-tab="tab2" class="qld__tab-content" role="tabpanel" aria-labelledby="tab2-button" id="tab2-content" tabindex="0">Tab 2 content goes here</div>
            <div data-tab="tab3" class="qld__tab-content" role="tabpanel" aria-labelledby="tab3-button" id="tab3-content" tabindex="0">Tab 3 content goes here</div>
            <div data-tab="tab4" class="qld__tab-content" role="tabpanel" aria-labelledby="tab4-button" id="tab4-content" tabindex="0">Tab 4 content goes here</div>
            <div data-tab="tab5" class="qld__tab-content" role="tabpanel" aria-labelledby="tab5-button" id="tab5-content" tabindex="0">Tab 5 content goes here</div>
        </div>
    </div>
</section>
{{/with}}